<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="action_object_provider" class="Civi\API\Provider\ActionObjectProvider">
            <tag name="event_subscriber"/>
        </service>

        <service id="spec_gatherer" class="Civi\API\Spec\SpecGatherer"/>

        <service id="contact_creation.spec_provider" class="Civi\API\Spec\Provider\ContactCreationSpecProvider">
            <tag name="spec_provider"/>
        </service>

        <service id="custom_group.pre_creation.subsciber" class="Civi\API\Event\Subscriber\CustomGroupPreCreationSubscriber">
            <tag name="event_subscriber"/>
        </service>

        <service id="custom_field.pre_creation.subsciber" class="Civi\API\Event\Subscriber\CustomFieldPreCreationSubscriber">
            <tag name="event_subscriber"/>
        </service>

        <service id="null_value.pre_creation.subscriber" class="Civi\API\Event\Subscriber\NullValueFormattingPreCreationSubscriber">
            <tag name="event_subscriber"/>
        </service>

        <service id="contact.pre_update.subscriber" class="Civi\API\Event\Subscriber\ContactPreUpdateSubscriber">
            <tag name="event_subscriber"/>
        </service>

    </services>
</container>
